{
  "name": "legends_of_runeterra_twitch_win_tracker",
  "version": "0.1.0",
  "main": "electron-server.js",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.668.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run dynamoDb\" ",
    "start-no-db": "npm run server",
    "start-prod": "SET NODE_ENV=production && npm run start-no-db",
    "server": "node server.js",
    "electron": "concurrently \"npm run electron-no-db\" \"npm run dynamoDb\" ",
    "electron-no-db": "electron electron-server.js",
    "dynamoDb": "cd dynamodb && java -Djava.library.path=./DynamoDBLocal_lib -jar DynamoDBLocal.jar -sharedDb",
    "deploy-history-lambda": "cd aws-lambda/getLoRHistoryByChannelId && serverless deploy",
    "invoke-history-lambda-prod": "cd aws-lambda/getLoRHistoryByChannelId && serverless invoke -f getLoRHistoryByChannelId --path local_request_data.json",
    "invoke-history-lambda": "cd aws-lambda/getLoRHistoryByChannelId && serverless invoke local -f getLoRHistoryByChannelId --path local_request_data.json"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^8.2.4"
  }
}
